---
import Layout from '../../layouts/Layout.astro';
import Hero from '../../components/Hero.astro';
import ServicesOverview from '../../components/ServicesOverview.astro';
import Technologies from '../../components/Technologies.astro';
import Process from '../../components/Process.astro';
import CTA from '../../components/CTA.astro';
import Breadcrumb from '../../components/Breadcrumb.astro';

// SEO metadata
const title = 'Logistics Software Solutions | EA Soft Lab - Supply Chain Management';
const description = 'Custom logistics software solutions for supply chain management, transportation, and warehouse operations.';
const structuredData = {
  type: 'service',
  data: {
    "@context": "https://schema.org",
    "@type": "Service",
    "name": "Logistics Software Development",
    "description": "Comprehensive logistics software solutions including supply chain management, fleet tracking, and logistics optimization tools",
    "url": "https://easoftlab.com/industries/logistics",
    "provider": {
      "@type": "Organization",
      "name": "EA Soft Lab"
    },
    "areaServed": "Worldwide",
    "serviceType": "Logistics Development",
    "offers": {
      "@type": "Offer",
      "description": "Supply chain management systems, fleet tracking solutions, warehouse management, and logistics optimization tools"
    }
  }
};
---

<Layout title={title} description={description} structuredData={structuredData}>
  <!-- Main Content with Continuous Dark Background -->
  <div class="relative overflow-hidden" style="background-color: hsl(230, 30%, 5%); background-image: radial-gradient(at 20% 20%, hsl(210, 60%, 20%) 0px, transparent 50%), radial-gradient(at 80% 80%, hsl(240, 50%, 15%) 0px, transparent 50%);">
    
    <!-- Breadcrumb Navigation -->
    <Breadcrumb 
      items={[
        { name: "Home", url: "https://easoftlab.com", position: 1 },
        { name: "Industries", url: "https://easoftlab.com/industries", position: 2 },
        { name: "Logistics", url: "https://easoftlab.com/industries/logistics", position: 3 }
      ]} 
    />
    
    <!-- Hero Section -->
    <div class="pt-24 pb-16 relative z-10" data-lazy-load>
      <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="text-center">
          <h1 class="text-4xl md:text-5xl font-bold text-white mb-6 mt-8 pt-4 opacity-0 transform translate-y-8 transition-all duration-1000 ease-out delay-200" data-lazy-animate>
            Logistics Software Development
          </h1>
          <p class="text-xl text-gray-300 max-w-3xl mx-auto mb-8 opacity-0 transform translate-y-8 transition-all duration-1000 ease-out delay-400" data-lazy-animate>
            Transform your logistics operations with innovative technology solutions
          </p>
          <p class="text-lg text-gray-400 max-w-2xl mx-auto mb-8 opacity-0 transform translate-y-8 transition-all duration-1000 ease-out delay-600" data-lazy-animate>
            From supply chain management to fleet tracking, we help logistics companies optimize operations and improve efficiency with cutting-edge software solutions.
          </p>
          <div class="opacity-0 transform translate-y-8 transition-all duration-1000 ease-out delay-800" data-lazy-animate>
            <a href="/contact" class="bg-gradient-to-r from-primary-500 to-accent-500 hover:from-primary-600 hover:to-accent-600 text-white px-8 py-4 rounded-xl text-lg font-semibold transition-all duration-300 transform hover:scale-105 shadow-2xl hover:shadow-primary-500/25 animate-bounce-slow">
              Start Your Logistics Project
            </a>
          </div>
        </div>
      </div>
    </div>

    <!-- Logistics Solutions -->
    <div class="py-16 relative z-10" data-lazy-load>
      <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="text-center mb-12 opacity-0 transform translate-y-8 transition-all duration-1000 ease-out" data-lazy-animate>
          <h2 class="text-3xl font-bold text-white mb-6">Logistics Solutions That Drive Efficiency</h2>
          <p class="text-lg text-gray-300">
            We specialize in creating powerful logistics software that helps companies optimize supply chains, track shipments, and improve operational efficiency.
          </p>
        </div>
        
        <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-8">
          <div class="bg-white/5 backdrop-blur-sm border border-white/10 rounded-xl p-6 hover:bg-white/10 transition-all duration-300 shadow-lg opacity-0 transform translate-y-8 transition-all duration-1000 ease-out hover:scale-105 hover:shadow-2xl" data-lazy-animate style="animation-delay: 200ms">
            <div class="w-12 h-12 bg-primary-500/20 rounded-lg flex items-center justify-center mb-4">
              <svg class="w-6 h-6 text-primary-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M20 7l-8-4-8 4m16 0l-8 4m8-4v10l-8 4m0-10L4 7m8 4v10M4 7v10l8 4"></path>
              </svg>
            </div>
            <h3 class="text-xl font-semibold text-white mb-3">Supply Chain Management</h3>
            <p class="text-gray-300">Comprehensive platforms for managing inventory, suppliers, and supply chain operations.</p>
          </div>

          <div class="bg-white/5 backdrop-blur-sm border border-white/10 rounded-xl p-6 hover:bg-white/10 transition-all duration-300 shadow-lg opacity-0 transform translate-y-8 transition-all duration-1000 ease-out hover:scale-105 hover:shadow-2xl" data-lazy-animate style="animation-delay: 400ms">
            <div class="w-12 h-12 bg-primary-500/20 rounded-lg flex items-center justify-center mb-4">
              <svg class="w-6 h-6 text-primary-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 20l-5.447-2.724A1 1 0 013 16.382V5.618a1 1 0 011.447-.894L9 7m0 13l6-3m-6 3V7m6 10l4.553 2.276A1 1 0 0021 18.382V7.618a1 1 0 00-1.447-.894L15 4m0 13V4m0 0L9 7"></path>
              </svg>
            </div>
            <h3 class="text-xl font-semibold text-white mb-3">Fleet Tracking & Management</h3>
            <p class="text-gray-300">Real-time fleet tracking systems with route optimization and driver management.</p>
          </div>

          <div class="bg-white/5 backdrop-blur-sm border border-white/10 rounded-xl p-6 hover:bg-white/10 transition-all duration-300 shadow-lg opacity-0 transform translate-y-8 transition-all duration-1000 ease-out hover:scale-105 hover:shadow-2xl" data-lazy-animate style="animation-delay: 600ms">
            <div class="w-12 h-12 bg-primary-500/20 rounded-lg flex items-center justify-center mb-4">
              <svg class="w-6 h-6 text-primary-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4"></path>
              </svg>
            </div>
            <h3 class="text-xl font-semibold text-white mb-3">Warehouse Management</h3>
            <p class="text-gray-300">Advanced warehouse management systems for inventory control and order fulfillment.</p>
          </div>

