---
import Layout from '../../layouts/Layout.astro';
import Breadcrumb from '../../components/Breadcrumb.astro';
import { getCompanyInfo, getService } from '../../lib/queries.js';
import SanityImage from '../../components/SanityImage.astro';
import RichText from '../../components/RichText.astro';

// Fetch CMS data
const companyInfo = await getCompanyInfo();
const serviceData = await getService('ui-ux-design');

// Fallback data if CMS is not available
const fallbackService = {
  title: "UI/UX Design Services",
  description: "Professional UI/UX design services that create intuitive, engaging user experiences. User research, wireframing, prototyping, and visual design for web and mobile applications.",
  shortDescription: "We create intuitive, engaging user experiences that delight users and drive business success through thoughtful design and user research.",
  features: [
    {
      title: "User Research",
      description: "Comprehensive user research to understand your target audience, their needs, and pain points."
    },
    {
      title: "Wireframing & Prototyping",
      description: "Create interactive wireframes and prototypes to test user flows and gather feedback early."
    },
    {
      title: "Visual Design",
      description: "Beautiful, consistent visual designs that enhance user experience and reflect your brand identity."
    }
  ],
  technologies: ["Figma", "Adobe XD", "Sketch", "InVision", "Protopie", "Framer"],
  pricing: {
    startingPrice: "$3,000",
    pricingModel: "Project-based",
    pricingDetails: "Pricing varies based on project scope and design complexity"
  },
  seo: {
    metaTitle: "UI/UX Design Services | EA Soft Lab - User Experience Design",
    metaDescription: "Professional UI/UX design services that create intuitive, engaging user experiences. User research, wireframing, prototyping, and visual design for web and mobile applications."
  }
};

// Use CMS data or fallback
const service = serviceData || fallbackService;
const company = companyInfo || { name: "EA Soft Lab" };

// SEO metadata
const title = 'UI/UX Design Services | EA Soft Lab - User-Centered Design';
const description = 'Professional UI/UX design services that create intuitive, engaging, and user-friendly digital experiences.';
---

<Layout title={title} description={description}>
  <!-- Main Content with Continuous Dark Background -->
  <div class="relative overflow-hidden" style="background-color: hsl(230, 30%, 5%); background-image: radial-gradient(at 20% 20%, hsl(210, 60%, 20%) 0px, transparent 50%), radial-gradient(at 80% 80%, hsl(240, 50%, 15%) 0px, transparent 50%);">
    
    <!-- Breadcrumb Navigation -->
    <Breadcrumb 
      items={[
        { name: "Home", url: "https://easoftlab.com", position: 1 },
        { name: "Services", url: "https://easoftlab.com/services", position: 2 },
        { name: "UI/UX Design", url: "https://easoftlab.com/services/ui-ux-design", position: 3 }
      ]} 
    />
    
    <!-- Hero Section -->
    <div class="pt-24 pb-16 relative z-10" data-lazy-load>
      <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="text-center">
          <h1 class="text-4xl md:text-5xl font-bold text-white mb-6 mt-8 pt-4 opacity-0 transform translate-y-8 transition-all duration-1000 ease-out delay-200" data-lazy-animate>
            UI/UX Design Services
          </h1>
          <p class="text-xl text-gray-300 max-w-3xl mx-auto mb-8 opacity-0 transform translate-y-8 transition-all duration-1000 ease-out delay-400" data-lazy-animate>
            Create intuitive, engaging, and user-centered digital experiences that drive engagement and conversion
          </p>
        </div>
      </div>
    </div>

    <!-- Services Overview -->
    <div class="py-16 relative z-10" data-lazy-load>
      <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="grid md:grid-cols-2 gap-12">
          <div class="opacity-0 transform translate-y-8 transition-all duration-1000 ease-out" data-lazy-animate style="animation-delay: 200ms">
            <h2 class="text-3xl font-bold text-white mb-6">What We Offer</h2>
            <p class="text-lg text-gray-300 mb-6">
              Our UI/UX design services focus on creating user-centered experiences that are both beautiful and functional. We combine research, creativity, and technical expertise to deliver designs that users love.
            </p>
            <ul class="space-y-3 text-gray-300">
              <li class="flex items-start">
                <span class="w-2 h-2 bg-primary-400 rounded-full mt-2 mr-3 flex-shrink-0"></span>
                User Research & Persona Development
              </li>
              <li class="flex items-start">
                <span class="w-2 h-2 bg-primary-400 rounded-full mt-2 mr-3 flex-shrink-0"></span>
                Information Architecture & Wireframing
              </li>
              <li class="flex items-start">
                <span class="w-2 h-2 bg-primary-400 rounded-full mt-2 mr-3 flex-shrink-0"></span>
                Interactive Prototyping & Testing
              </li>
              <li class="flex items-start">
                <span class="w-2 h-2 bg-primary-400 rounded-full mt-2 mr-3 flex-shrink-0"></span>
                Visual Design & Brand Integration
              </li>
            </ul>
          </div>

          <div class="opacity-0 transform translate-y-8 transition-all duration-1000 ease-out" data-lazy-animate style="animation-delay: 400ms">
            <div class="bg-white/5 backdrop-blur-sm border border-white/10 rounded-xl p-8">
              <h3 class="text-2xl font-bold text-white mb-4">Our Design Process</h3>
              <div class="space-y-4">
                <div class="flex items-center">
                  <div class="w-8 h-8 bg-primary-600 rounded-full flex items-center justify-center mr-4">
                    <span class="text-white font-bold text-sm">1</span>
                  </div>
                  <div>
                    <h4 class="text-white font-semibold">Research</h4>
                    <p class="text-gray-300 text-sm">Understanding users and business goals</p>
                  </div>
                </div>
                <div class="flex items-center">
                  <div class="w-8 h-8 bg-primary-600 rounded-full flex items-center justify-center mr-4">
                    <span class="text-white font-bold text-sm">2</span>
                  </div>
                  <div>
                    <h4 class="text-white font-semibold">Design</h4>
                    <p class="text-gray-300 text-sm">Creating wireframes and prototypes</p>
